      "kind": "Route",
      "apiVersion": "v1",
      "metadata": {
        "name": "lazar-gui",
        "annotations": {
          "kubernetes.io/tls-acme": "true"
        }
      },
      "spec": {
        "host": "https://lazar-gui.prod.openrisknet.org",
        "to": {
          "kind": "Service",
          "name": "lazar-gui"
        },
        "tls": {
          "termination": "edge"
        }
      },
      "status": {}
    }
  ],
  "parameters": [],
  "labels": {
    "template": "lazar-template"
  }
}
